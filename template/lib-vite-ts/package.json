{
  "name": "vite-lib",
  "version": "0.0.2",
  "author": "chengyu",
  "license": "MIT",
  "main": "./dist/bundle.umd.js",
  "module": "./dist/bundle.es.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/bundle.es.js",
      "require": "./dist/bundle.umd.js"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build-watch": "vite build --watch",
    "ts:check": "tsc --noEmit -p ./tsconfig.json",
    "lint:check": "eslint --ext .js,.ts,.vue,.jsx,.tsx package",
    "lint:fix": "eslint --fix --ext .js,.ts,.vue,.jsx,.tsx package"
  },
  "dependencies": {
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "@vitejs/plugin-vue": "^2.3.0",
    "eslint": "^8.12.0",
    "eslint-plugin-vue": "^8.5.0",
    "fs-extra": "^10.0.1",
    "hash-generator": "^0.1.0",
    "rollup-plugin-visualizer": "^5.6.0",
    "sass": "^1.49.10",
    "sass-loader": "^12.6.0",
    "typescript": "^4.5.4",
    "vite": "^2.9.0",
    "vite-plugin-dts": "^1.1.1",
    "vue-tsc": "^0.29.8"
  }
}

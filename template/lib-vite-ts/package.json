{
  "name": "venus2d-map-provider",
  "version": "0.0.1",
  "license": "MIT",
  "author": "chengyu",
  "description": "a map provider plugin for venus3d base cesium",
  "main": "./dist/venus2d-map-provider.umd.js",
  "module": "./dist/venus2d-map-provider.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/venus2d-map-provider.es.js",
      "require": "./dist/venus2d-map-provider.umd.js"
    }
  },
  "scripts": {
    "build-types": "node ./script/types-excutor.js",
    "build": "vue-tsc --noEmit && vite build && npm run build-types",
    "typecheck": "vue-tsc --noEmit"
  },
  "dependencies": {
    "cesium": "^1.87.1",
    "vue": "^3.2.22"
  },
  "devDependencies": {
    "@types/node": "^16.11.9",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@vitejs/plugin-vue": "^1.9.4",
    "@vue/compiler-sfc": "^3.2.22",
    "eslint": "^8.3.0",
    "eslint-plugin-vue": "^8.1.1",
    "typescript": "^4.5.2",
    "vite": "^2.4.4",
    "vue-tsc": "^0.29.6"
  }
}

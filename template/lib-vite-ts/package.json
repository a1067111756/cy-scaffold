{
  "name": "vite-lib",
  "version": "0.0.2",
  "author": "chengyu",
  "license": "MIT",
  "main": "./dist/bundle.umd.js",
  "module": "./dist/bundle.es.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/bundle.es.js",
      "require": "./dist/bundle.umd.js"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build-watch": "vite build --watch",
    "ts:check": "tsc --noEmit -p ./tsconfig.json",
    "lint:check": "eslint --ext .js,.ts,.vue,.jsx,.tsx package",
    "lint:fix": "eslint --fix --ext .js,.ts,.vue,.jsx,.tsx package"
  },
  "dependencies": {
    "vue": "^3.3.2"
  },
  "devDependencies": {
    "@types/node": "^20.1.4",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "@vitejs/plugin-vue": "^4.2.3",
    "eslint": "^8.40.0",
    "eslint-plugin-vue": "^9.12.0",
    "fs-extra": "^11.1.1",
    "hash-generator": "^0.1.0",
    "rollup-plugin-visualizer": "^5.9.0",
    "sass": "^1.62.1",
    "sass-loader": "^13.2.2",
    "typescript": "^4.8.4",
    "vite": "^4.3.5",
    "vite-plugin-dts": "^2.3.0",
    "vue-tsc": "^1.6.5"
  }
}
